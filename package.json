{
    "name": "aws-ag",
    "version": "1.0.0",
    "description": "AWS Access Grants - CLI tool for managing Azure AD security groups within AWS IAM Identity Center integration",
    "main": "dist/index.js",
    "bin": {
        "aws-ag": "dist/cli.js"
    },
    "scripts": {
        "build": "tsc",
        "dev": "ts-node src/cli.ts",
        "start": "node dist/cli.js",
        "clean": "rm -rf dist",
        "prebuild": "npm run clean",
        "watch": "tsc --watch",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "lint": "eslint src/**/*.ts",
        "lint:fix": "eslint src/**/*.ts --fix"
    },
    "keywords": [
        "aws",
        "azure",
        "sso",
        "identity",
        "access-grants",
        "cli",
        "typescript"
    ],
    "author": "",
    "license": "MIT",
    "dependencies": {
        "@aws-sdk/client-identitystore": "^3.490.0",
        "@aws-sdk/client-organizations": "^3.490.0",
        "@aws-sdk/client-sso-admin": "^3.490.0",
        "@azure/msal-node": "^2.6.0",
        "@microsoft/microsoft-graph-client": "^3.0.7",
        "@types/uuid": "^10.0.0",
        "chalk": "^4.1.2",
        "commander": "^11.1.0",
        "dotenv": "^16.3.1",
        "inquirer": "^8.2.6",
        "ora": "^5.4.1",
        "uuid": "^13.0.0"
    },
    "devDependencies": {
        "@types/inquirer": "^9.0.7",
        "@types/jest": "^29.5.8",
        "@types/node": "^20.10.0",
        "@typescript-eslint/eslint-plugin": "^6.13.0",
        "@typescript-eslint/parser": "^6.13.0",
        "eslint": "^8.54.0",
        "jest": "^29.7.0",
        "ts-jest": "^29.1.1",
        "ts-node": "^10.9.1",
        "typescript": "^5.3.0"
    },
    "engines": {
        "node": ">=18.0.0"
    }
}
